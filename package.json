{
  "name": "colinhas",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "device:android": "expo run:android --device",
    "device:ios": "expo run:ios --device",
    "prebuild": "expo prebuild",
    "build:dev:android": "eas build -p android --profile development --local",
    "build:dev:ios": "eas build -p ios --profile development --local",
    "build:prod:android": "eas build -p android",
    "build:prod:ios": "eas build -p ios",
    "build:prod:web": "expo export -p web",
    "test": "npm run lint && npm run tsc",
    "lint": "eslint .",
    "tsc": "tsc"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.0",
    "@react-native-async-storage/async-storage": "^2.1.0",
    "eas-cli": "^14.2.0",
    "expo": "^52.0.23",
    "expo-constants": "~17.0.3",
    "expo-dev-client": "~5.0.7",
    "expo-document-picker": "~13.0.1",
    "expo-image": "~2.0.3",
    "expo-keep-awake": "~14.0.1",
    "expo-linking": "~7.0.3",
    "expo-router": "~4.0.15",
    "expo-status-bar": "~2.0.0",
    "firebase": "^10.12.2",
    "opensheetmusicdisplay": "^1.8.9",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.5",
    "react-native-gesture-handler": "~2.21.2",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "15.8.0",
    "react-native-web": "~0.19.13",
    "react-string-replace": "^1.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.3.12",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "firebase-tools": "^13.13.1",
    "prettier": "^3.3.2",
    "typescript": "^5.7.2"
  },
  "private": true
