{
  "name": "colinhas",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "device:android": "expo run:android --device",
    "device:ios": "expo run:ios --device",
    "prebuild": "expo prebuild",
    "build:dev:android": "eas build -p android --profile development --local",
    "build:dev:ios": "eas build -p ios --profile development --local",
    "build:prod:android": "eas build -p android",
    "build:prod:ios": "eas build -p ios",
    "build:prod:web": "expo export -p web",
    "test": "lint && tsc",
    "lint": "eslint .",
    "tsc": "tsc"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "eas-cli": "^10.0.3",
    "expo": "~51.0.14",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.19",
    "expo-linking": "~6.3.1",
    "expo-router": "~3.5.16",
    "expo-status-bar": "~1.12.1",
    "firebase": "^10.12.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.2",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.10"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.45",
    "eslint": "^8.57.0",
    "eslint-config-expo": "^7.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "firebase-tools": "^13.13.1",
    "prettier": "^3.3.2",
    "typescript": "^5.1.3"
  },
  "private": true
}
